{
    "name": "quip-cli",
    "description": "A Command Line Interface for the Quip Live Apps platform",
    "version": "1.0.0-alpha.31",
    "author": "Quip",
    "bin": {
        "quip-cli": "./bin/run",
        "qla": "./bin/run"
    },
    "bugs": "https://github.com/quip/quip-apps/issues",
    "dependencies": {
        "@oclif/command": "^1.6.1",
        "@oclif/config": "^1.15.1",
        "@oclif/plugin-help": "^3.1.0",
        "inquirer": "^7.2.0",
        "mkdirp": "^1.0.4",
        "ncp": "^2.0.0",
        "open": "^7.0.4",
        "prettier": "^2.0.5",
        "tslib": "^1.13.0"
    },
    "devDependencies": {
        "@oclif/dev-cli": "^1.22.2",
        "@oclif/test": "^1.2.6",
        "@types/inquirer": "^6.5.0",
        "@types/jest": "^26.0.4",
        "@types/mkdirp": "^1.0.1",
        "@types/ncp": "^2.0.4",
        "@types/nock": "^11.1.0",
        "@types/node": "^10.17.26",
        "@types/open": "^6.2.1",
        "@types/prettier": "^2.0.2",
        "chai": "^4.2.0",
        "jest": "^26.1.0",
        "mockdate": "^3.0.2",
        "ts-jest": "^26.1.1",
        "ts-node": "^8.10.2",
        "typescript": "^3.9.5"
    },
    "engines": {
        "node": ">=8.0.0"
    },
    "files": [
        "/bin",
        "/lib",
        "/npm-shrinkwrap.json",
        "/oclif.manifest.json"
    ],
    "homepage": "https://github.com/quip/quip-apps#readme",
    "keywords": [
        "oclif",
        "quip",
        "live",
        "apps"
    ],
    "license": "Apache-2.0",
    "main": "lib/index.js",
    "oclif": {
        "commands": "./lib/commands",
        "bin": "qla",
        "plugins": [
            "@oclif/plugin-help"
        ]
    },
    "repository": {
        "type": "git",
        "url": "git+ssh://git@github.com/quip/quip-apps.git"
    },
    "scripts": {
        "postpack": "rm -f oclif.manifest.json",
        "prepack": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
        "test": "jest",
        "test:cover": "jest --coverage",
        "test:watch": "jest --watch",
        "version": "oclif-dev readme && git add README.md"
    },
    "types": "lib/index.d.ts"
}
